{
  "name": "website-user",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/website-user/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/website-user",
        "configFile": "apps/website-user/vite.config.ts"
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "website-user:build",
        "port": 4300,
        "host": "localhost",
        "configFile": "apps/website-user/vite.config.ts"
      }
    }
  }
}
